.container2 .content2 { color:#000000; background-color:#FFFFFF; font-size:12px; }
.dcb_pop { position:absolute; top:0px; left:0px; border:1px dashed red; background:white; width:500px; height:34px; z-index:1; -ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=80)"; filter:alpha(opacity=80); -khtml-opacity:0.8; -moz-opacity:0.8; opacity:0.8;}
.dcb_pop .form { position:absolute; top:4px; left:10px; }
.dcb_pop .email { position:absolute; top:6px; left:160px; }
.dcb_pop .closer { position:absolute; top:1px; right:5px; display:block; font-weight:bold; color:red; text-decoration:none; }

